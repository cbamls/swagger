<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns="http://www.springframework.org/schema/beans"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">

	<!-- leaf.properties 文件加载请查看“spring-beans.xml”文件 -->

    <!--请求转发 forward 类型逻辑视图-->
	<bean id="tDocContact" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/WEB-INF/swagger/tDocContact.jsp" />
	</bean>
	<bean id="tDocTag" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/WEB-INF/swagger/tDocTag.jsp" />
	</bean>
	<bean id="tDoc" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/WEB-INF/swagger/tDoc.jsp" />
	</bean>
	<bean id="tDocDefine" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/WEB-INF/swagger/tDocUserDifine.jsp" />
	</bean>
	<bean id="inter" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/WEB-INF/swagger/interInfo.jsp" />
	</bean>
	<bean id="interList" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/WEB-INF/swagger/interList.jsp" />
	</bean>
	<bean id="datasource" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/WEB-INF/swagger/dataSource.jsp" />
	</bean>
	<bean id="interInfoModify" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/WEB-INF/swagger/interInfoModify.jsp" />
	</bean>
	<bean id="editReq" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/WEB-INF/swagger/editReq.jsp" />
	</bean>
	<bean id="editRes" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/WEB-INF/swagger/editRes.jsp" />
	</bean>
	<bean id="tDocUserDifine" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/WEB-INF/swagger/tDocUserDifine.jsp" />
	</bean>
	<bean id="init" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/welcome.jsp" />
	</bean>
	<bean id="reflect_index" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/index_2.jsp" />
	</bean>
	<bean id="readme" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/WEB-INF/swagger/readme.jsp" />
	</bean>
</beans>