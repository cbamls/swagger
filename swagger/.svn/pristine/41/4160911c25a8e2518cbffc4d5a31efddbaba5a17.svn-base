<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns="http://www.springframework.org/schema/beans"
	   xmlns:util="http://www.springframework.org/schema/util"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
      http://www.springframework.org/schema/util
      http://www.springframework.org/schema/util/spring-util-4.1.xsd">

	<!-- leaf.properties 文件加载请查看“spring-beans.xml”文件 -->

    <!--url 路径et参数进行执行者映射-->
    <util:map id="urlRoute" map-class="java.util.HashMap">
        <entry key="swagger" value="Swagger"/>
        <entry key="solve" value="manifest"/>
        <entry key="tty1" value="manifest"/>
        <entry key="tty2" value="dataSource"/>
    </util:map>
    <!--每个模块的默认执行者-->
    <util:map id="urlRouteDefault" map-class="java.util.HashMap">
        <entry key="swagger" value="Swagger"/>
    </util:map>
</beans>